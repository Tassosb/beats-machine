<h1>Sign In</h1>

<form action="/session" method="post">
  <label>Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>
  <label>Password
    <input type="password" name="user[password]" value="<%= @user.password %>">
  </label>
  <input type="submit" value="Sign Up">
</form>
